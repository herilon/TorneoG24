@page
@model Torneo.App.Frontend.Pages.DTs.EditModel
@{
}
<h1>Actualizar director técnico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="dt.Id"></label>
        <input asp-for="dt.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="dt.Nombre"></label>
        <input asp-for="dt.Nombre" class="form-control" placeholder="Ingrese el nombre del DT">
    </div>
    <div class="form-group">
        <label asp-for="dt.Documento"></label>
        <input asp-for="dt.Documento" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="dt.Telefono"></label>
        <input asp-for="dt.Telefono" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary btn-block">Actualizar director técnico</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>